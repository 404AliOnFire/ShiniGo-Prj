                  <ImageView fitHeight="30.0" fitWidth="25.0" preserveRatio="true">
                     <image>
                        <Image url="@../Images/icons8-save-48.png" />
                     </image>
                  </ImageView>
               </graphic>
               <font>
                  <Font name="Cairo Regular" size="12.0" />
               </font>
            </Button>
            <Label alignment="CENTER" prefHeight="30.0" prefWidth="150.0" text="تغيير الاسم">
