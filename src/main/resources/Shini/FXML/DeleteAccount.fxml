<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Shini.Controller">
   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="400.0">
         <children>
            <Label alignment="CENTER" prefHeight="50.0" prefWidth="200.0" text="هل أنت متأكد؟">
               <font>
                  <Font name="Cairo Black Bold" size="24.0" />
               </font>
            </Label>
            <Label alignment="CENTER" prefHeight="50.0" prefWidth="251.0" text="لتأكيد حذف حسابك,انقر فوق &quot;تأكيد&quot;">
               <font>
                  <Font name="Cairo Black" size="14.0" />
               </font>
            </Label>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="cancle" mnemonicParsing="false" prefHeight="35.0" prefWidth="70.0" stylesheets="@../Styles/CancleButton.css" text="الغاء" />
                  <Button fx:id="confirm" mnemonicParsing="false" prefHeight="35.0" prefWidth="70.0" stylesheets="@../Styles/ConfirmButton.css" text="تأكيد">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
